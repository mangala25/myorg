try {
	
	node
	{  		
		stage('clean-workspace') {
		    echo 'current working direcotiy is :' 
		    echo pwd
		    sh 'mvn --version'
		}
		stage('git-latest') {
		    echo 'git-latest'
		    def rtGradle = Artifactory.newGradleBuild()
    		    env.JAVA_HOME="${tool 'jdk8u162'}"
		}
		stage('mvn-build') {
		    echo 'mvn-build'
		}
		stage('mvn-junit') {
		    echo 'mvn-junit'
		}
		stage('mvn-sonar') {
		    echo 'mvn-sonar'
		}
		stage('pcf-push'){
		    echo 'pcf-push'
		}
	
	}
}
catch (e){
        echo "${e}"
}
